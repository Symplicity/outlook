{
    "name": "symplicity/outlook",
    "type": "library",
    "description": "Outlook",
    "license": "proprietary",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.symplicity.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ramsey/uuid" : "^3.8.0",
        "guzzlehttp/guzzle": "^6.2",
        "monolog/monolog" : "^1.17",
		"myclabs/php-enum": "^1.6.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.5",
        "phpunit/phpunit": "^5.7"
    },
    "minimum-stability": "stable",
    "config": {
        "optimize-autoloader": true,
        "discard-changes": "stash",
        "notify-on-install": false,
        "process-timeout": 3000
    },
    "autoload": {
        "psr-4": {
            "Symplicity\\Outlook\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Symplicity\\Outlook\\Test\\": "tests/"
        }
    }
}
